{
    "name": "lariele/order",
    "description": "Managing Orders package",
    "keywords": ["lariele", "laravel", "livewire", "orders"],
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel Cepec",
            "email": "daniel.cepec@gmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "lariele/form": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lariele\\Order\\OrderServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Lariele\\Order\\": "src/Order",
            "Database\\": "src/Order/Database"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
